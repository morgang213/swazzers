{
  "name": "ems-supply-tracker",
  "version": "1.0.0",
  "description": "Cloud-based medical supply inventory management system for EMS agencies",
  "main": "backend/src/server.js",
  "scripts": {
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "install:all": "npm run install:backend && npm run install:frontend",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "dev": "docker-compose up",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "build": "npm run build:backend && npm run build:frontend",
    "start": "docker-compose up -d",
    "stop": "docker-compose down",
    "db:migrate": "cd backend && npm run migrate:latest",
    "db:seed": "cd backend && npm run seed:run",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "clean": "docker-compose down -v",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["ems", "inventory", "medical-supplies", "healthcare", "saas"],
  "author": "",
  "license": "ISC"
}

